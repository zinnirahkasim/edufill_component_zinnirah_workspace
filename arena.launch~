<launch>
  <!-- send table urdf to param server -->
  <param name="table_description" command="$(find xacro)/xacro.py brsu-c025-arena.urdf.xacro" />

  <!-- push table_description to factory and spawn robot in gazebo -->
  <node name="spawn_arena" pkg="gazebo" type="spawn_model" args="-urdf -param table_description -z 0.01 -model arena" respawn="false" output="screen" />
</launch>
